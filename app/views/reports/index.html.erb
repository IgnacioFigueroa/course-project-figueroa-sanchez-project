<div class="d-lg-flex p-3 justify-content-md-center">
  <div class="jumbotron" style="width:100%">
    <br>
    <table class="table table-striped table-hover">
      <thead>
        <th>Title</th>
        <th>Author e-mail</th>
        <th>Creation Date</th>
        <th>Number of reports</th>
      </thead>
      <tbody>
      <%@reports.each do |report| %>
        <tr>
          <td><%=report["post_title"] %></td>
          <td><%=report["author"] %></td>
          <td><%=report["date"] %></td>
          <td><%=report["reports_count"] %></td>
        </tr>
      <% end %>
      </tbody>
    </table>
  </div>
</div>