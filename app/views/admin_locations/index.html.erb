<p id="notice"><%= notice %></p>

<h1>Admin Locations</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Location</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @admin_locations.each do |admin_location| %>
      <tr>
        <td><%= admin_location.user %></td>
        <td><%= admin_location.location %></td>
        <td><%= link_to 'Show', admin_location %></td>
        <td><%= link_to 'Edit', edit_admin_location_path(admin_location) %></td>
        <td><%= link_to 'Destroy', admin_location, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Admin Location', new_admin_location_path %>
